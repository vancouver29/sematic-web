{
	"name": "FilterSpecialPages",
	"author": "Leonid Verhovskij",
	"url": "https://www.mediawiki.org/wiki/Extension:FilterSpecialPages",
	"descriptionmsg": "filterspecialpages-desc",
	"version": "0.9",
