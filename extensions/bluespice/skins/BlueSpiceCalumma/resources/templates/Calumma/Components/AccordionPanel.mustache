<div class="panel-group bs-accordion" id="{{id}}" role="tablist" aria-multiselectable="true">
	{{#panels}}
	<div class="panel panel-default bs-accordion-panel" id="{{id}}">
		<div class="panel-heading" role="tab" id="heading-{{id}}">
			<h4 class="panel-title">
				<a role="button" data-toggle="collapse" data-parent="#{{accordionId}}" href="#collapse-{{id}}" aria-expanded="{{#in}}true{{/in}}" aria-controls="collapse-{{id}}">
					{{{title}}}
				</a>
			</h4>
		</div>
		<div id="collapse-{{id}}" class="panel-collapse collapse {{#in}}in{{/in}}" role="tabpanel" aria-labelledby="heading-{{id}}">
			<div class="panel-body">
				{{{body}}}
			</div>
		</div>
	</div>
	{{/panels}}
</div>
